{
  "retryCount": 3,
  "timeout": "3000",
  "fallbackRetryDelay": "1000",
  "aliveStatusCodes": [200, 206],
  "httpHeaders": [
    {
      "urls": ["*"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check/3.12.2)",
        "Accept": "text/html'"
      }
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^https://crates.io/*"
    }
  ]
}
